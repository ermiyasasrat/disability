
<link rel="stylesheet" href="{{ 'component-quick-edit-cart.css' | asset_url }}" media="print" onload="this.media='all'">

<div class="scoder-popup scoder-edit-cart-popup customPopup-large" data-edit-cart-popup id="scoder-edit-cart-popup" data-url-style-edit-cart="{{ 'component-quick-edit-cart.css' | asset_url }}" data-url-script-edit-cart="{{ 'variants-edit-cart.js' | asset_url }}">
    <div class="scoder-popup-wrapper">
    	<div class="scoder-popup-edit-header">
            <button type="button" class="scoder-popup-close clearfix" data-close-edit-cart aria-label="{{ 'general.common.close' | t }}">{% render 'icon-close' %} {{ 'general.common.close' | t }}</button>
            <h5 class="scoder-popup-title text-left">{{ 'sections.cart.edit_cart_title' | t }}</h5>
    	</div>
        <div class="scoder-popup-content scoder-popup-scroll custom-scrollbar"></div>
    </div>
</div>